<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="JgA03TitleDAO">




    <select id="selectList"  parameterType="gsis.com.cms.datainfo.jgAtitle.vo.JewA03TiileDataVO"   resultType="gsis.com.cms.datainfo.jgAtitle.vo.JewA03TiileDataVO">
        SELECT
             DATA_YEAR
            ,DATA_GB
            ,CDM_DATA1
            ,CDM_DATA2
            ,CDM_DATA3
            ,CDM_DATA4
            ,CDM_DATA5
            ,CDM_DATA6
            ,CDM_DATA7
            ,CDM_DATA8
            ,CDM_DATA9
            ,CDM_DATA10
            ,CDM_DATA11
            ,CDM_DATA12
            ,CDM_DATA13
            ,CDM_DATA14
            ,CDM_DATA15
            ,CDM_DATA16
            ,CDM_DATA17
            ,CDM_DATA18
            ,REG_ID
            ,REG_DT

        FROM JEW_A_03_TIILE_DATA
        WHERE 1=1
        ORDER BY DATA_YEAR asc

    </select>



    <insert id="insert">


         INSERT INTO JEW_A_03_TIILE_DATA(

                     DATA_YEAR
                    ,DATA_GB
                    ,CDM_DATA1
                    ,CDM_DATA2
                    ,CDM_DATA3
                    ,CDM_DATA4
                    ,CDM_DATA5
                    ,CDM_DATA6
                    ,CDM_DATA7
                    ,CDM_DATA8
                    ,CDM_DATA9
                    ,CDM_DATA10
                    ,CDM_DATA11
                    ,CDM_DATA12
                    ,CDM_DATA13
                    ,CDM_DATA14
                    ,CDM_DATA15
                    ,CDM_DATA16
                    ,CDM_DATA17
                    ,CDM_DATA18
                    ,REG_ID
                    ,REG_DT

                    ) VALUES (


                     #{dataYear}
                    ,#{dataGb}
                    ,#{cdmData1}
                    ,#{cdmData2}
                    ,#{cdmData3}
                    ,#{cdmData4}
                    ,#{cdmData5}
                    ,#{cdmData6}
                    ,#{cdmData7}
                    ,#{cdmData8}
                    ,#{cdmData9}
                    ,#{cdmData10}
                    ,#{cdmData11}
                    ,#{cdmData12}
                    ,#{cdmData13}
                    ,#{cdmData14}
                    ,#{cdmData15}
                    ,#{cdmData16}
                    ,#{cdmData17}
                    ,#{cdmData18}
                    ,#{regId}
                    ,NOW()


                 )

    </insert>


    <delete id="delete">

      DELETE FROM JEW_A_03_TIILE_DATA
      WHERE 1=1
      AND DATA_YEAR = #{dataYear}
      AND DATA_GB =#{dataGb}



    </delete>

</mapper>