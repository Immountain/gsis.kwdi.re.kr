    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.0.xsd">

    <!--wj schedule자동증가-->
    <bean name="WjFestivityScheduleIdGnrService" class="egovframework.rte.fdl.idgnr.impl.EgovTableIdGnrServiceImpl" destroy-method="destroy">
        <property name="dataSource" ref="egov.dataSource" />
        <property name="blockSize"  value="10"/>
        <property name="table"      value="INFO_IDS"/>
        <property name="tableNameFieldName"  value="IDGEN_TABLE_NM"/>
        <property name="nextIdFieldName" value="IDGEN_NEXT_ID"/>
